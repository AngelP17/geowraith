{
  "name": "geowraith-backend",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node --enable-source-maps dist/index.js",
    "build": "tsc -p tsconfig.json",
    "lint": "tsc --noEmit -p tsconfig.json",
    "test": "tsx --test src/**/*.test.ts",
    "benchmark:accuracy": "tsx src/benchmarks/accuracyBenchmark.ts",
    "build:dataset": "tsx src/scripts/buildReferenceDataset.ts --target=50000"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "exifr": "^7.1.3",
    "express": "^4.21.2",
    "onnxruntime-node": "^1.22.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2"
  }
}
